        </TableView>
    </center>
    <bottom>
        <HBox spacing="10" alignment="CENTER">
            <Button text="Adicionar"/>
            <Button text="Editar"/>
            <Button text="Excluir"/>
        </HBox>
    </bottom>
</BorderPane>